{
  "name": "@open-source-bazaar/web-site",
  "version": "1.0.0",
  "description": "Open Source Bazaar web-site",
  "private": true,
  "scripts": {
    "e": "pnpx @dotenvx/dotenvx run -f .env.personal.local -- pnpm",
    "prepare": "husky",
    "install": "pnpx git-utility download https://github.com/Open-Source-Bazaar/key-vault main Open-Source-Bazaar.github.io  ||  true",
    "postinstall": "next typegen",
    "dev": "next dev --webpack",
    "build": "next build --webpack",
    "start": "next start",
    "test": "lint-staged  &&  tsc --noEmit"
  },
  "dependencies": {
    "@koa/router": "^15.1.1",
    "@mdx-js/loader": "^3.1.1",
    "@mdx-js/react": "^3.1.1",
    "@next/mdx": "^16.1.1",
    "core-js": "^3.47.0",
    "echarts-jsx": "^0.6.0",
    "file-type": "^21.2.0",
    "idea-react": "^2.0.0-rc.13",
    "jsonwebtoken": "^9.0.3",
    "koa": "^3.1.1",
    "koa-jwt": "^4.0.4",
    "koajax": "^3.1.2",
    "license-filter": "^0.2.5",
    "marked": "^17.0.1",
    "mime": "^4.1.0",
    "mobx": "^6.15.0",
    "mobx-github": "^0.6.2",
    "mobx-i18n": "^0.7.2",
    "mobx-lark": "^2.6.3",
    "mobx-react": "^9.2.1",
    "mobx-react-helper": "^0.5.1",
    "mobx-restful": "^2.1.4",
    "mobx-restful-table": "^2.6.3",
    "mobx-strapi": "^0.8.1",
    "next": "^16.1.1",
    "next-pwa": "^5.6.0",
    "next-ssr-middleware": "^1.1.0",
    "open-react-map": "^0.9.1",
    "react": "^19.2.3",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.2.3",
    "react-typed-component": "^1.0.6",
    "remark-frontmatter": "^5.0.0",
    "remark-mdx-frontmatter": "^5.2.0",
    "web-utility": "^4.6.4",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/plugin-transform-typescript": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@cspell/eslint-plugin": "^9.4.0",
    "@eslint/js": "^9.39.2",
    "@next/eslint-plugin-next": "^16.1.1",
    "@open-source-bazaar/china-ngo-database": "^0.6.0",
    "@softonus/prettier-plugin-duplicate-remover": "^1.1.2",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/koa": "^3.0.1",
    "@types/next-pwa": "^5.6.9",
    "@types/node": "^22.19.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "less": "^4.5.1",
    "less-loader": "^12.3.0",
    "lint-staged": "^16.2.7",
    "next-with-less": "^3.0.1",
    "prettier": "^3.7.4",
    "prettier-plugin-css-order": "^2.1.2",
    "sass": "^1.97.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.51.0"
  },
  "resolutions": {
    "mobx-react-helper": "$mobx-react-helper",
    "next": "$next"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "core-js",
      "less",
      "sharp",
      "unrs-resolver"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid",
    "printWidth": 100,
    "plugins": [
      "prettier-plugin-css-order",
      "@softonus/prettier-plugin-duplicate-remover"
    ]
  },
  "lint-staged": {
    "*.{html,md,less,json,yml,js,mjs,ts,tsx}": "prettier --write"
  }
}
